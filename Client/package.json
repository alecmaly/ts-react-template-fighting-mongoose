{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "React TS Template.",
  "main": "index.js",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack --mode production",
    "start": "set NODE_ENV=development && webpack-dev-server --mode development --config webpack.config.js",
    "type-check": "tsc",
    "test": "jest --coverage",
    "jest-clear": "jest --clearCache",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "dockerize": "docker build --rm -f Dockerfile -t alecmaly/dockerized-app . && docker push alecmaly/dockerized-app"
  },
  "author": "Alec J Maly",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
